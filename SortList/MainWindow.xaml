<Window x:Class="SortList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="32" Margin="98,111,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="137"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="32" Margin="98,148,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="137"/>
        <Button Content="Add" HorizontalAlignment="Left" Height="69" Margin="252,111,0,0" VerticalAlignment="Top" Width="95" Click="Button_Click"/>

    </Grid>
</Window>
